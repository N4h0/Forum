@model Room

<h2>Update Room</h2>

<form asp-action="UpdateCategory">
    <input type="hidden" asp-for="RoomId" />

    <div>
        <label asp-for="RoomName"></label>
        <input asp-for="RoomName" class="form-control" />
        <span aps-validation-for="Name" class="text-danger"></span>


    </div>
    <button type="submit" class="btn btn-primary">Save changes</button>
    <a asp-action="Table" class="btn btn-secondry">Cancel</a>

</form>